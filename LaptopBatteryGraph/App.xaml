<Application x:Class="LaptopBatteryGraph.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LaptopBatteryGraph"
             StartupUri="MainWindow.xaml"
             
             xmlns:conv="clr-namespace:LaptopBatteryGraph.Converters"
             xmlns:vm="clr-namespace:LaptopBatteryGraph.ViewModels"
             >
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator" />

            <conv:Converter_ExpandedTimeSpan x:Key="ExpandedTimeSpan" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
